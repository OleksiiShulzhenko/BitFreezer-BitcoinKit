CONFIGURATION_BUILD_DIR = ${PODS_CONFIGURATION_BUILD_DIR}/Secp256k1Kit.swift
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_TARGET_SRCROOT}/Secp256k1Kit/Libraries/include"
LIBRARY_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Secp256k1Kit.swift/Secp256k1Kit/Libraries/lib" "${PODS_TARGET_SRCROOT}/Secp256k1Kit/Libraries/lib"
OTHER_LDFLAGS = $(inherited) -l"secp256k1"
OTHER_SWIFT_FLAGS = $(inherited) -D COCOAPODS
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}
PODS_TARGET_SRCROOT = ${PODS_ROOT}/Secp256k1Kit.swift
PRODUCT_BUNDLE_IDENTIFIER = org.cocoapods.${PRODUCT_NAME:rfc1034identifier}
SKIP_INSTALL = YES
USE_RECURSIVE_SCRIPT_INPUTS_IN_SCRIPT_PHASES = YES
